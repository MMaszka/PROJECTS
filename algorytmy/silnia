function silnia(ile)
{	var silnia = 1
	var ile 
	var licznik = 1
	
			if(ile>=0)
		{
			while(ile>=licznik)
			{
				silnia*=licznik;
				licznik+=1;
				
			}
			return silnia;	
		}
		return "niemozliwe"
		
}



